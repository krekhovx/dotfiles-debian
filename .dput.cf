[mentors]
fqdn = mentors.debian.net
incoming = /upload
method = https
progress_indicator = 2
allow_unsigned_uploads = 0
# Allow uploads for UNRELEASED packages
allowed_distributions = .*

[ftp-master]
fqdn = ftp.upload.debian.org
incoming = /pub/UploadQueue/
login = anonymous
allow_dcut = 1
method = ftp
progress_indicator = 2
allow_unsigned_uploads = 0
